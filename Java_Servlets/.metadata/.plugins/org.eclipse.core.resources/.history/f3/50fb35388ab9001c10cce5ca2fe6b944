package com.javalec.ascent.dto;

public class ADtoPaging {
	//paging
	public int totalCount; //전체 게시글 수 
	public int currentPage; // 현재 페이
	public int totalPage; // 전체 페이지 수 

	//pagingation 
	private int displayPage =5; // 한 페이지에 몇 개의 페이지 
	private int displayRow = 5; // 한 페이지에 몇 개의 로우 
	private int beginPage; // 페이징 시작 페이지수 
	private int endPage; // 페이징 종료 페이지수 
	
}
